commands:
  01_download_zip:
    command: "curl -L -o /tmp/model.zip https://virtualscale-cdn.s3.us-east-2.amazonaws.com/vosk/models/med/vosk-model-en-us-daanzu-20200905.zip"
  02_create_directory:
    command: "mkdir -p /var/app/current/src/models/med"
  03_unzip_model:
    command: "unzip -q /tmp/model.zip -d /tmp"
  04_move_model:
    command: "mv /tmp/vosk-model-en-us-daanzu-20200905 /var/app/current/src/models/med/"
  05_cleanup_zip:
    command: "rm /tmp/model.zip"